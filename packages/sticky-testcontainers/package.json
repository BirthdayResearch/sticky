{
  "name": "@birthdayresearch/sticky-testcontainers",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc -b ./tsconfig.build.json",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "test": "jest"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "@birthdayresearch"
    ]
  },
  "jest": {
    "displayName": "test",
    "preset": "@birthdayresearch/sticky-turbo-jest"
  },
  "dependencies": {
    "testcontainers": "^9.0.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-kms": "^3.220.0",
    "@birthdayresearch/sticky-turbo-jest": "workspace:*",
    "@birthdayresearch/sticky-typescript": "workspace:*",
    "@types/uuid": "^8.3.4",
    "node-fetch": "2.6.7",
    "@types/node-fetch": "2.6.2"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
